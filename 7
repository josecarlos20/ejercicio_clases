const contador = {
  valor: 0,
  ultimoComandoGuardado: "",

  reset: function() {
    this.ultimoComandoGuardado = "reset";
    this.valor = 0;
  },

  inc: function() {
    this.ultimoComandoGuardado = "incremento";
    this.valor++;
  },

  dec: function() {
    this.ultimoComandoGuardado = "decremento";
    this.valor--;
  },

  valorActual: function() {
    if (arguments.length === 1) {
      this.ultimoComandoGuardado = "actualizacion";
      this.valor = arguments[0];
    }
    return this.valor;
  },

  ultimoComando: function() {
    return this.ultimoComandoGuardado;
  }
};

contador.valorActual(10);
contador.inc();
contador.inc();
contador.dec();
contador.inc();

console.log(contador.valorActual()); // Debería mostrar 12
console.log(contador.ultimoComando()); // Debería mostrar "incremento"
